{
  "authors": [
    {
      "homepage": "https://graffino.com/",
      "name": "Graffino"
    }
  ],
  "config": {
    "vendor-dir": "composer/vendor"
  },
  "description": "Ninja. Website building webpack flow.",
  "extra": {
    "installer-paths": {
      "dist-wp/wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "dist-wp/wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    },
    "wordpress-install-dir": "dist-wp"
  },
  "license": "MIT",
  "minimum-stability": "dev",
  "name": "graffino/ninja",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://connect.advancedcustomfields.com/index.php?p=pro&a=download&k={%PLUGIN_ACF_KEY}&t={%version}"
        },
        "name": "advanced-custom-fields/advanced-custom-fields-pro",
        "require": {
          "composer/installers": "^1.10"
        },
        "type": "wordpress-plugin",
        "version": "5.9.3"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://wpml.org/?download=1097589&user_id={%PLUGIN_WPML_ID}&subscription_key={%PLUGIN_WPML_KEY}&version={%version}"
        },
        "name": "wpml/acfml",
        "require": {
          "composer/installers": "^1.10"
        },
        "type": "wordpress-plugin",
        "version": "1.7.3"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://wpml.org/?download=6088&user_id={%PLUGIN_WPML_ID}&subscription_key={%PLUGIN_WPML_KEY}&version={%version}"
        },
        "name": "wpml/sitepress-multilingual-cms",
        "require": {
          "composer/installers": "^1.10"
        },
        "type": "wordpress-plugin",
        "version": "4.3.19"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://wpml.org/?download=7474&user_id={%PLUGIN_WPML_ID}&subscription_key={%PLUGIN_WPML_KEY}&version={%version}"
        },
        "name": "wpml/wpml-media-translation",
        "require": {
          "composer/installers": "^1.10"
        },
        "type": "wordpress-plugin",
        "version": "2.5.6"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://wpml.org/?download=6092&user_id={%PLUGIN_WPML_ID}&subscription_key={%PLUGIN_WPML_KEY}&version={%version}"
        },
        "name": "wpml/wpml-string-translation",
        "require": {
          "composer/installers": "^1.10"
        },
        "type": "wordpress-plugin",
        "version": "3.0.14"
      },
      "type": "package"
    },
    {
      "package": {
        "dist": {
          "type": "zip",
          "url": "https://wpml.org/?download=7474&user_id={%PLUGIN_WPML_ID}&subscription_key={%PLUGIN_WPML_KEY}&version={%version}"
        },
        "name": "wpml/wpml-translation-management",
        "require": {
          "composer/installers": "^1.10"
        },
        "type": "wordpress-plugin",
        "version": "2.9.9"
      },
      "type": "package"
    }
  ],
  "require": {
    "advanced-custom-fields/advanced-custom-fields-pro": "^5.9",
    "johnpbloch/wordpress": "^5.7",
    "johnpbloch/wordpress-core-installer": "^2.0",
    "vlucas/phpdotenv": "^4.1",
    "wp-cli/wp-cli": "^2.4",
    "wpackagist-plugin/ajax-load-more": "^5.4",
    "wpackagist-plugin/complianz-gdpr": "^4.9",
    "wpackagist-plugin/custom-post-type-maker": "^1.1",
    "wpackagist-plugin/debug-bar": "^1.1",
    "wpackagist-plugin/force-regenerate-thumbnails": "^2.0",
    "wpackagist-plugin/google-site-kit": "^1.28",
    "wpackagist-plugin/redirection": "^5.0",
    "wpackagist-plugin/safe-svg": "^1.9",
    "wpackagist-plugin/smtp-mailer": "1.1.1",
    "wpackagist-plugin/wordpress-seo": "^16.0",
    "wpackagist-plugin/wp-super-cache": "^1.7",
    "wpml/acfml": "^1.7",
    "wpml/sitepress-multilingual-cms": "^4.3",
    "wpml/wpml-media-translation": "^2.5",
    "wpml/wpml-string-translation": "^3.0"
  },
  "require-dev": {
    "composer/installers": "^1.10",
    "dealerdirect/phpcodesniffer-composer-installer": "^0.7.1",
    "ffraenz/private-composer-installer": "^5.0.1",
    "phpcompatibility/phpcompatibility-wp": "^2.1",
    "squizlabs/php_codesniffer": "^3.5",
    "wp-coding-standards/wpcs": "^2.3"
  },
  "type": "project"
}
