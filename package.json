    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2",
    "yesno": "^0.3.1"
  },
  "dependencies": {
    "jump.js": "^1.0.2",
    "swiper": "^5.1.0",
    "vanilla-lazyload": "^12.1.0"
  }
