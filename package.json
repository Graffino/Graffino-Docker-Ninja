{
  "name": "ninja",
  "version": "1.0.3",
  "description": "Website building webpack flow we use at Graffino.",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev.js --env.NODE_ENV=development",
    "build": "webpack --config webpack.prod.js",
    "wp": "webpack --progress --watch --config webpack.wordpress.js",
    "wp-setup": "yarn && composer install --prefer-dist && composer update --prefer-dist && composer update && node wordpress/scripts/db-init.js",
    "wp-migrate": "node wordpress/scripts/db-migrate.js",
    "wp-dump": "node wordpress/scripts/db-dump.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint-code": "eslint src -c .eslintrc --ext js",
    "lint-style": "stylelint 'src/styles/**/*.scss'",
    "fix-code": "prettier-eslint --write 'src/**/*.js' --config .eslintrc",
    "fix-style": "prettier-stylelint --write 'src/**/*.{css,scss}'",
    "composer-install": "composer install --prefer-dist && composer update",
    "deploy": "git fetch --all; git reset --hard origin/develop; yarn; yarn run build",
    "bump-minor": "node bin/bump.js -- minor",
    "bump-major": "node bin/bump.js -- major",
    "bump-patch": "node bin/bump.js -- patch"
  },
  "repository": "git@github.com:Graffino/ninja.git",
  "author": "team@graffino.com",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@degjs/module-loader": "^5.1.0",
    "autoprefixer": "^9.6.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-polyfill": "^6.26.0",
