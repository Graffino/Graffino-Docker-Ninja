{
  "name": "ninja",
  "version": "1.0.11",
  "description": "Graffino Ninja. Website building webpack flow we use at Graffino.",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --config webpack.dev.js --env.NODE_ENV=development",
    "build": "webpack --config webpack.prod.js",
    "wp": "yarn wp:dev",
    "wp:dev": "node bin/sync-uploads --no-confirm && webpack --progress --watch --config webpack.wordpress.js",
    "wp:setup": "yarn && composer install --prefer-dist && composer update && node bin/db-init.js && node bin/sync-uploads.js --no-confirm && node bin/clean-wp.js",
    "wp:build": "yarn && composer install --prefer-dist && composer update && node bin/sync-uploads --no-confirm && webpack --config webpack.wordpress.prod.js",
    "wp:migrate": "node bin/db-migrate.js",
    "wp:dump": "node bin/db-dump.js",
    "wp:update": "composer update && node bin/sync-uploads.js --no-confirm",
    "wp:sync:uploads": "node bin/sync-uploads.js",
    "wp:clean": "node bin/clean-wp.js",
    "test": "yarn cypress run",
    "test:interactive": "yarn cypress open",
    "test:regression": "yarn cypress run --spec \"cypress/integration/regression/**/*.spec.js\"  --reporter cypress-image-snapshot/reporter --env failOnSnapshotDiff=false --browser chrome",
    "test:regression:approve": "docker run -it -v $PWD:/e2e -w /e2e -e CYPRESS_baseUrl=http://host.docker.internal:3000 -e CYPRESS_updateSnapshots=true cypress/included:4.0.2 run --browser chrome",
    "test:regression:strict": "yarn cypress run --spec \"cypress/integration/regression/**/*.spec.js\" --reporter cypress-image-snapshot/reporter --browser chrome",
    "lint:js": "eslint src -c .eslintrc --ext js",
    "lint:scss": "stylelint 'src/styles/**/*.scss'",
    "fix:js": "prettier-eslint --write 'src/**/*.js' --config .eslintrc",
    "fix:scss": "prettier-stylelint --write 'src/**/*.{css,scss}'",
    "composer:install": "composer install --prefer-dist && composer update",
    "deploy": "git fetch --all; git reset --hard origin/develop; yarn; yarn run build",
    "bump": "node bin/bump.js"
  },
  "repository": "git@github.com:Graffino/ninja.git",
  "author": "team@graffino.com",
  "license": "MIT",
